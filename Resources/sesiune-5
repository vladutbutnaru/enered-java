		char[][] harta = new char[8][8];

		for (int i = 0; i < 8; i++) {
			harta[0][i] = '0';
			harta[i][5] = '0';
		}
		harta[1][0] = '0';
		harta[1][1] = '0';
		harta[1][2] = '1';
		harta[1][3] = '0';
		harta[1][4] = '1';
