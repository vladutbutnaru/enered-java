		harta[7][4] = '0';
		harta[7][5] = '0';
		harta[7][6] = '0';
		harta[7][7] = '1';

		int xCascaval = 5;
		int yCascaval = 6;
		int xMouse = 7;
		int yMouse = 2;
		int optiuni = 0;
		int saltX[] = { 0, 0, 0, 0, 0, 0, 0, 0, 0 };
		int saltY[] = { 0, 0, 0, 0, 0, 0, 0, 0, 0 };
		int progresiv = 1;
		harta[xCascaval][yCascaval] = 'c';
		harta[xMouse][yMouse] = 's';
		for (int i = 0; i < 8; i++) {
			for (int j = 0; j < 8; j++) {
				System.out.print(harta[i][j] + " ");
			}
			System.out.println("");

		}
		System.out.println("");

		while (yCascaval != yMouse || xCascaval != xMouse) { // cand soarecele ajunge la cascaval bucla se va opri

			// facem o functie pentru intoarcerea la o locatie cand nu mai sunt optiuni

			if (xMouse > 0 && yMouse < 7)
				if (harta[xMouse - 1][yMouse] == '0' && harta[xMouse][yMouse + 1] == '0') {
					optiuni++;
					saltX[optiuni] = xMouse;
					saltY[optiuni] = yMouse;
				}
			if (xMouse > 0 && yMouse < 7 && xMouse < 7)
				if (harta[xMouse - 1][yMouse] == '0' && harta[xMouse][yMouse + 1] == '0'
						&& harta[xMouse + 1][yMouse] == '0') {
					optiuni++;
					saltX[optiuni] = xMouse;
					saltY[optiuni] = yMouse;
				}
			if (xMouse > 0 && yMouse < 7 && xMouse < 7 && yMouse > 1)

				if (harta[xMouse - 1][yMouse] == '0' && harta[xMouse][yMouse + 1] == '0'
						&& harta[xMouse + 1][yMouse] == '0' && harta[xMouse][yMouse - 1] == '0') {
					optiuni++;
					saltX[optiuni] = xMouse;
					saltY[optiuni] = yMouse;
				}

			// Mai jos sunt mutarile face doar o mutare pe repetitie
			if (xMouse > 0 && (harta[xMouse - 1][yMouse] == '0' || harta[xMouse - 1][yMouse] == 'c')) { // mutare sus
				harta[xMouse][yMouse] = '-';
				xMouse--;
				harta[xMouse][yMouse] = 's';
			} else {
				if (yMouse < 7 && (harta[xMouse][yMouse + 1] == '0' || harta[xMouse][yMouse + 1] == 'c')) { // mutare
																											// dreapta
					harta[xMouse][yMouse] = '-';
					yMouse++;
					harta[xMouse][yMouse] = 's';
				} else {
					if (xMouse < 7 && (harta[xMouse + 1][yMouse] == '0' || harta[xMouse + 1][yMouse] == 'c')) { // mutare
																												// jos
						harta[xMouse][yMouse] = '-';
						xMouse++;
						harta[xMouse][yMouse] = 's';
					} else {
						if (yMouse > 0 && (harta[xMouse][yMouse - 1] == '0' || harta[xMouse][yMouse - 1] == 'c')) { // mutare
																													// stanga
							harta[xMouse][yMouse] = '-';
							yMouse--;
							harta[xMouse][yMouse] = 's';
						} else {
							harta[xMouse][yMouse] = '-';
							xMouse = saltX[progresiv]; // daca nu poate sa mearga in nici o directie
							yMouse = saltY[progresiv]; // coordonatele vor lua prima valoare de unde avea mai mult de o
														// optiune
							progresiv++;

						}
					}
				}
			} // e extra ca sa pot vedea pas cu pas cum lucreaza
			for (int i = 0; i < 8; i++) {
				for (int j = 0; j < 8; j++) {
					System.out.print(harta[i][j] + " ");
				}
				System.out.println("");

			}
			System.out.println("");
		}
		// afisare dupa parcurgere e mai jos

		for (int i = 0; i < 8; i++) {
			for (int j = 0; j < 8; j++) {
				System.out.print(harta[i][j] + " ");
			}
			System.out.println("");

		}

		System.out.println(xMouse + " " + yMouse);
		System.out.println(xCascaval + " " + yCascaval);

		for (int i = 0; i < 8; i++) {
			for (int j = 0; j < 8; j++) {
				System.out.print(harta[i][j] + " ");
			}
			System.out.println();
		}

	}

